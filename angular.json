{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "shell": {
      "projectType": "application",
      "root": "projects/shell",
      "sourceRoot": "projects/shell/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/shell",
            "index": "projects/shell/src/index.html",
            "main": "projects/shell/src/main.ts",
            "polyfills": ["zone.js", "es-module-shims"],
            "tsConfig": "projects/shell/tsconfig.app.json",
            "assets": [{ "glob": "**/*", "input": "projects/shell/public" }],
            "styles": ["projects/shell/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": { "optimization": true },
            "development": { "sourceMap": true }
          },
          "defaultConfiguration": "development"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": { "browserTarget": "shell:build:development", "port": 4200 }
        }
      }
    },
    "agenda": {
      "projectType": "application",
      "root": "projects/agenda",
      "sourceRoot": "projects/agenda/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/agenda",
            "index": "projects/agenda/src/index.html",
            "main": "projects/agenda/src/main.ts",
            "polyfills": ["zone.js", "es-module-shims"],
            "tsConfig": "projects/agenda/tsconfig.app.json",
            "assets": [{ "glob": "**/*", "input": "projects/agenda/public" }],
            "styles": ["projects/agenda/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": { "optimization": true },
            "development": { "sourceMap": true }
          },
          "defaultConfiguration": "development"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": { "browserTarget": "agenda:build:development", "port": 4201 }
        }
      }
    },
    "financeiro": {
      "projectType": "application",
      "root": "projects/financeiro",
      "sourceRoot": "projects/financeiro/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/financeiro",
            "index": "projects/financeiro/src/index.html",
            "main": "projects/financeiro/src/main.ts",
            "polyfills": ["zone.js", "es-module-shims"],
            "tsConfig": "projects/financeiro/tsconfig.app.json",
            "assets": [{ "glob": "**/*", "input": "projects/financeiro/public" }],
            "styles": ["projects/financeiro/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": { "optimization": true },
            "development": { "sourceMap": true }
          },
          "defaultConfiguration": "development"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": { "browserTarget": "financeiro:build:development", "port": 4202 }
        }
      }
    }
